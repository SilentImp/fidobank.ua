body>header
  background-color #FFF
  .wrapper
    max-width 1024px
    margin 0 auto
    height 96px
    overflow hidden
    h1
      height 96px
      width 304px
      background url("../images/logotype/header-logotype.png") 16px 50% no-repeat
      overflow hidden
      text-indent -9999px
      float left
    .contacts
      float right
      color #00316f
      width 416px
      padding 37px 12px 0 0
      font-size 24px
      font-family 'Plumb-Medium'
      .title
        display inline-block
        vertical-align top
        margin-right 32px
      a
        text-decoration none
        color #808285
        display inline-block
        vertical-align top
        width 160px
        span
          font-family 'Plumb-Regular'
          display block
          font-size 10px
          line-height 12px
  